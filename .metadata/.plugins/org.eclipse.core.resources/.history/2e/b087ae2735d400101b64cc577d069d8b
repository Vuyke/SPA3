package Klase;

public class Main {
	public static void main(String args[]) {
		try {
			Graph g = new Graph("tinyG.txt");
			System.out.println(g);
			System.out.println(g.komponenta(9));
			System.out.println(g.komponente());
			
			System.out.println(g.put(0, 6));
			System.out.println(g.put(0, 7));
			
			System.out.println(g.konture());
			
			System.out.println(g.brojGrana());
			g.dodajGranu(0, 9);
			System.out.println(g.brojGrana());
			g.dfsIter();
			g.ispisPuta(3, 2);
		} catch (Exception e) {
			e.printStackTrace();
		}
		
	}
}
