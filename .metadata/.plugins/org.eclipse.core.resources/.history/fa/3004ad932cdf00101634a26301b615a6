package Klase;

import java.util.Set;

public class Graph extends GraphA {
	
	public Graph(String fileName) {
		super(fileName);
	}

	@Override
	public void dodajGranu(int x, int y) {
		Set<Integer> trenSusedi = susedi.get(x);
		grane -= trenSusedi.size();
		trenSusedi.add(y);
		susedi.get(y).add(x);
		grane += trenSusedi.size();
	}
}